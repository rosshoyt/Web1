@model Web1.Models.HomeViewModel
@{
    ViewBag.Title = "Home Page";
}


<div class="jumbotron">
    <h1>Ross Hoyt</h1>
</div>
<img src="~/Content/Img/oregoncoast.jpg" class="img-thumbnail" />
<div class="row">
    <div class="col text-center">
        <h2>Number of Phones</h2>
        <h3>@Model.PhoneCount</h3>
    </div>

    <div class="col text-center">
        <h2>Number of Users</h2>
        <h3>@Model.UserCount</h3>
    </div>

    <div class="col text-center">
        <h2>Last Access Date</h2>
        <h3>@Model.LastAcessDate.ToShortDateString()</h3>
    </div>

</div>